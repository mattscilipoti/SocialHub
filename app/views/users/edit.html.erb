<%= form_for @user do |f| %>

<p>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</p>


  <p>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </p>


<p>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
</p>


<p>
  <%= f.label :email %>
  <%= f.text_field :email %>
</p>


<p>
  <%= f.label :password %>
  <%= f.password_field :password %>
</p>

<p>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
</p>


  <%= f.submit %>

<% end %>
